<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <!-- Sequences for sale.order -->
        <record id="seq_inv_number" model="ir.sequence">
            <field name="name">Invoice Number</field>
            <field name="code">invoice.sequence.shridhan</field>
            <field name="prefix"></field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
	    <field name="implementation">no_gap</field>
        </record>

        <record id="seq_credit_note_number" model="ir.sequence">
            <field name="name">Credit Note Number</field>
            <field name="code">creditnote.sequence.shridhan</field>
            <field name="prefix"></field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
	    <field name="implementation">no_gap</field>
        </record>

        <record id="seq_bill_number" model="ir.sequence">
            <field name="name">Bill Number</field>
            <field name="code">bill.sequence.shridhan</field>
            <field name="prefix">VB-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">no_gap</field>
        </record>

        <record id="seq_refund_number" model="ir.sequence">
            <field name="name">Refund Number</field>
            <field name="code">refund.sequence.shridhan</field>
            <field name="prefix">Ref-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">no_gap</field>
        </record>
        <record id="invoice_number_seq" model="ir.sequence">
            <field name="name">Inter state invoice number</field>
            <field name="code">inter.state.invoice.number</field>
            <field name="prefix">0</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
	    <field name="implementation">no_gap</field>
        </record>
     </data>

</odoo>
